{
  "name": "deploysage",
  "version": "0.1.0",
  "description": "Deploy Sage",
  "main": "server.js",
  "engines": {
    "node": "5.3.0",
    "npm": "3.3.12"
  },
  "scripts": {
    "build:clean": "rm app/assets/webpack/*",
    "build:prod:client": "(cd client && npm run build:prod:client --silent)",
    "build:dev:client": "(cd client && npm run build:dev:client --silent)",
    "postinstall": "cd client && npm install",
    "tdd": "(cd client && watch npm test)",
    "test": "(cd client && npm test)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deploysage/deploysage.git"
  },
  "keywords": [
    "elm"
  ],
