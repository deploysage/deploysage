{
  "preset": "airbnb",
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "excludeFiles": [
    "app/bundles/**",
    "assets/**",
    "build/**",
    "elm-stuff/**",
    "node_modules/**"
  ],
  "esprima": "babel-jscs",
  "maximumLineLength": 120,
  "validateQuoteMarks": {
    "mark": "'",
    "escape": true,
    "ignoreJSX": true
  }
}
