{
  "name": "deploysage",
  "version": "0.1.0",
  "description": "Deploy Sage",
  "main": "server.js",
  "engines": {
    "node": "5.3.0",
    "npm": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deploysage/deploysage.git"
  },
  "keywords": [
    "react"
  ],
  "author": "thewoolleyman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deploysage/deploysage/issues"
  },
  "homepage": "https://github.com/deploysage/deploysage",
  "scripts": {
    "test": "NODE_PATH=./app mocha --compilers js:babel-core/register --require ./app/libs/test/testHelper.js --require ./app/libs/test/testNullCompiler.js 'app/**/*.spec.@(js|jsx)'",
    "test:debug": "npm run test -- --debug-brk",
    "start": "node server-express.js",
    "build:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:dev:client": "babel-node server-rails-hot.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "build:test:client": "webpack -w --config webpack.client.rails.build.config.js",
    "lint": "npm run eslint && npm run jscs",
    "eslint": "eslint --max-warnings 1 --ext .js,.jsx .",
    "jscs": "jscs --verbose ."
  },
  "dependencies": {
    "autoprefixer": "^6.1.2",
    "axios": "^0.7.0",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap-loader": "^1.0.0-rc",
    "bootstrap-sass": "^3.3.5",
    "css-loader": "^0.23.0",
    "es5-shim": "^4.3.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "history": "^1.13.1",
    "humps": "^1.0.0",
    "immutable": "^3.7.5",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "jsuri": "^1.3.1",
    "loader-utils": "^0.2.11",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "mirror-creator": "^0.0.1",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.7",
    "normalizr": "^1.4.0",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-on-rails": "3.0.6",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "reqwest": "^2.0.4",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.1",
    "sass-resources-loader": "0.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "yamljs": "^0.2.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "body-parser": "^1.14.1",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jscs": "^2.7.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.4",
    "react-transform-hmr": "^1.0.1",
    "webpack-dev-server": "^1.12.1"
  }
}
