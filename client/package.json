{
  "name": "deploysage",
  "version": "0.1.0",
  "description": "Deploy Sage",
  "main": "server.js",
  "engines": {
    "node": "5.3.0",
    "npm": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deploysage/deploysage.git"
  },
  "keywords": [
    "elm"
  ],
  "author": "thewoolleyman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deploysage/deploysage/issues"
  },
  "homepage": "https://github.com/deploysage/deploysage",
  "scripts": {
    "build:prod.client": "NODE_ENV=production webpack --config webpack.static.config.js",
    "build:dev:client": "babel-node server-hot.js",
    "postinstall": "`npm bin`/elm-package install -y",
    "pretest": "`npm bin`/elm-make app/bundles/Test/Suite.elm --output app/bundles/Test/Suite.js",
    "tdd": "watch npm test",
    "test": "node app/bundles/Test/Suite.js"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "elm": "0.17.1",
    "elm-hot-loader": "^0.3.2",
    "elm-webpack-loader": "^3.0.1",
    "webpack": "^1.12.8"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "jscs": "^2.7.0",
    "webpack-dev-server": "^1.12.1"
  }
}
